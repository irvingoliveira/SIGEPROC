$(document).ready(function(){
    $('#requerenteTxt').blur(function(){
        var valor = $(this).val();
        $.post('/processos/getrequerenteinfo',{data: valor},function(ret){
            var obj = jQuery.parseJSON(ret);
            $('#dddTxt').val(obj.ddd);
            $('#telefoneTxt').val(obj.telefone);
            
            $('#numeroTxt').val(obj.documento);
            $('#digitoTxt').val(obj.digito);
            $('#emissaoDt').val(obj.emissao);
            $('#orgaoEmissorTxt').val(obj.orgaoEmissor);
        });
    });
});
