$(document).ready(function(){
    $('input[type=radio][name=requerenteRd]').change(function(){
        var valor = $(this).val();
        if(valor==0){
            $("#secretariaSlct").prop('disabled', true);
            $("#setorSlct").prop('disabled', true);
        } else{
            $("#secretariaSlct").removeAttr('disabled');
            $("#setorSlct").removeAttr('disabled');
        }
    });
});
